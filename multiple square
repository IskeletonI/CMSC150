public static void main(String[] args){
        Scanner scan = new Scanner(System.in);

        System.out.print("Enter integer between 1 - 10: ");
        int n = scan.nextInt();
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <=n; j++){
                System.out.print(j * i + "\t");
            }
            System.out.println("");
        }
        
    }
